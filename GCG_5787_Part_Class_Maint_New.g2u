Program.Sub.ErrorMsg.End

Program.Sub.createDisplayDT.Start
f.Intrinsic.Control.Try
	'Load tables.
	f.ODBC.Connection!con.OpenCompanyConnection
	 	f.Data.DataTable.CreateFromSQL("displayClassDT", "con", "Select * from GCG_5787_PART_CLASS", true)
	 	f.Data.DataTable.CreateFromSQL("displayTypeDT", "con", "Select * from GCG_5787_PART_TYPE", true)
	f.ODBC.Connection!con.Close
	'Create display table for part class
	f.Data.DataView.Create("displayClassDT", "displayClassDV", 22)
	gui.F_Maintenance.GsGcPartClass.AddGridviewFromDataview("displayClassGV", "displayClassDT", "displayClassDV")
	gui.F_Maintenance.GsGcPartClass.MainView("displayClassGV")
	'Create display table for part Type
	f.Data.DataView.Create("displayTypeDT", "displayTypeDV", 22)
	gui.F_Maintenance.GsGcPartType.AddGridviewFromDataview("displayTypeGV", "displayTypeDT", "displayTypeDV")
	gui.F_Maintenance.GsGcPartType.MainView("displayTypeGV")
f.Intrinsic.Control.Catch
	f.Intrinsic.Control.CallSub(ErrorMsg, CurrentSub, v.Ambient.CurrentSubroutine)
f.Intrinsic.Control.EndTry
Program.Sub.createDisplayDT.End

