Program.Sub.cmdNewType_Click.Start
v.Local.sRet.Declare
v.Local.iCnt.Declare
f.Intrinsic.Control.Try
	'Get Classes
	f.ODBC.Connection!con.OpenCompanyConnection
	 	f.ODBC.Connection!con.ExecuteAndReturn("Select distinct class_id from GCG_5787_PART_CLASS", v.Local.sRet)
	f.ODBC.Connection!con.Close
	'Split return to get an Array
	f.Intrinsic.String.Split(v.Local.sRet, "#$#", v.Local.sRet)
	'Clean Dropdown
	gui.F_PartType.ddl1.ClearItems
	'Loop through array
	f.Intrinsic.Control.For(v.Local.iCnt,  0, v.Local.sRet.UBound, 1)
		gui.F_PartType.ddl1.AddItem(v.Local.sRet(v.Local.iCnt))
	f.Intrinsic.Control.Next(v.Local.iCnt)
	
	gui.F_PartType..Visible(True)
f.Intrinsic.Control.Catch
	f.Intrinsic.Control.CallSub(ErrorMsg, CurrentSub, v.Ambient.CurrentSubroutine)
f.Intrinsic.Control.EndTry
Program.Sub.cmdNewType_Click.End

