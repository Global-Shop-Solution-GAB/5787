		gui.F_Maintenance.txt_D_SafetyQty.Text(v.DataTable.tempDefaultsDT(0).SAFETY_STOCK!FieldValTrim)
		gui.F_Maintenance.txt_D_ReorderPoint.Text(v.DataTable.tempDefaultsDT(0).REORDERPOINT!FieldValTrim)
		gui.F_Maintenance.txt_D_Buyer.Text(v.DataTable.tempDefaultsDT(0).BUYER!FieldValTrim)
		gui.F_Maintenance.txt_D_LeadDays.Text(v.DataTable.tempDefaultsDT(0).LEAD_DAYS!FieldValTrim)
		gui.F_Maintenance.txt_D_AmtCost.Text(v.DataTable.tempDefaultsDT(0).AMT_COST!FieldValTrim)
		gui.F_Maintenance.txt_D_MRC.Text(v.DataTable.tempDefaultsDT(0).MRC!FieldValTrim)
