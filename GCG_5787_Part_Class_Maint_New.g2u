Program.Sub.cmdSaveNewClass_Click.Start
v.Local.sPartClass.Declare
v.Local.sDescription.Declare
v.Local.sQuality.Declare

v.Local.sSQL.Declare
f.Intrinsic.Control.Try
	'Set variables with information from textboxes
	v.Local.sPartClass.Set(v.Screen.F_PartClass!txt_PC_PartClass.Text)
	v.Local.sDescription.Set(v.Screen.F_PartClass!txt_PC_Description.Text)
	v.Local.sQuality.Set(v.Screen.F_PartClass!txt_PC_Quality.Text)
	
	'Check for empty data.
	'Check part class is unique
	'Save into datatable.
	f.Intrinsic.String.Build("Insert into  GCG_5787_PART_CLASS (CLASS_ID, CLASS_DESC, CLASS_SPEC) Values ('{0}', '{1}', '{2}')", v.Local.sPartClass, v.Local.sDescription, v.Local.sQuality, v.Local.sSQL)
	f.ODBC.Connection!con.OpenCompanyConnection
	 	f.ODBC.Connection!con.Execute(v.Local.sSQL)
	f.ODBC.Connection!con.Close
	'Empty Text Fields
	gui.F_PartClass.txt_PC_PartClass.Text("")
	gui.F_PartClass.txt_PC_Description.Text("")
	gui.F_PartClass.txt_PC_Quality.Text("")
	'Reload Tables.
	f.Intrinsic.Control.CallSub(reloadTables)
f.Intrinsic.Control.Catch
	f.Intrinsic.Control.CallSub(ErrorMsg, CurrentSub, v.Ambient.CurrentSubroutine)
f.Intrinsic.Control.EndTry
Program.Sub.cmdSaveNewClass_Click.End

Program.Sub.reloadTables.Start
f.Intrinsic.Control.Try
'Load tables.
	f.ODBC.Connection!con.OpenCompanyConnection
	 	f.Data.DataTable.CreateFromSQL("tmpDisplayClassDT", "con", "Select * from GCG_5787_PART_CLASS", true)
	 	f.Data.DataTable.CreateFromSQL("tmpDisplayTypeDT", "con", "Select * from GCG_5787_PART_TYPE", true)
	f.ODBC.Connection!con.Close
	'Merge Table Part Class
	f.Data.DataTable.DeleteRow("displayClassDT")
	f.Data.DataTable.AcceptChanges("displayClassDT")
	f.Data.DataTable.Merge("tmpDisplayClassDT", "displayClassDT", false, 2)
	f.Data.DataTable.Close("tmpDisplayClassDT")
	'Merge Table Part Type
	f.Data.DataTable.DeleteRow("displayTypeDT")
	f.Data.DataTable.AcceptChanges("displayTypeDT")
	f.Data.DataTable.Merge("tmpDisplayTypeDT", "displayTypeDT", false, 2)
	f.Data.DataTable.Close("tmpDisplayTypeDT")
f.Intrinsic.Control.Catch
	f.Intrinsic.Control.CallSub(ErrorMsg, CurrentSub, v.Ambient.CurrentSubroutine)
f.Intrinsic.Control.EndTry
Program.Sub.reloadTables.End

